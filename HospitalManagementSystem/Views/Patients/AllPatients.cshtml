@model IEnumerable<HospitalManagementSystem.Models.Patients.PatientsListingModel>





@foreach(var patient in Model)
{
    <div class="row mt-60">
        <div class="col-md-6">
            <h3>@patient.Name</h3>
            
            <img src="@patient.ImagePath" width="200px">
            <a href="/Patients/Details/@patient.Id" class="btn btn-default">Full Profile</a>
            </div>
        </div>
}
 @